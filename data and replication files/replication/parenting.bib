	Journal = {Journal of Child and Family Studies},
	Keywords = {Authoritarian,Authoritative,Child and School Psychology,Japanese version of Parental Authority Questionnaire,Later mental health,Permissive,Social Sciences, general,Sociology, general},
	Language = {en},
	Month = feb,
	Number = {2},
	Pages = {293-302},
	Shorttitle = {The Impact of Authoritative, Authoritarian, and Permissive Parenting Styles on Children's Later Mental Health in Japan},
