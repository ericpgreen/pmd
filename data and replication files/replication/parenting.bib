	File = {Full Text PDF:/Users/ericpgreen/Library/Application Support/Zotero/Profiles/ma5l0ga2.default/zotero/storage/IJ8949Z4/Mills et al. - 2011 - Child Abuse and Neglect and Cognitive Function at .pdf:application/pdf;Snapshot:/Users/ericpgreen/Library/Application Support/Zotero/Profiles/ma5l0ga2.default/zotero/storage/K77T2Q3V/4.html:text/html},
	Journal = {Pediatrics},
	Keywords = {abuse,cognitive function,developmental outcomes,longitudinal study,neglect},
	Language = {en},
	Month = jan,
	Number = {1},
	Pages = {4-10},
	Shorttitle = {Child Abuse and Neglect and Cognitive Function at 14 Years of Age},
	Title = {Child Abuse and Neglect and Cognitive Function at 14 Years of Age: Findings From a Birth Cohort},
	Urldate = {2014-05-08},
