	Journal = {Social Science \& Medicine},
	Keywords = {{Cluster-randomized trial}, {Discrimination}, {HIV/AIDS}, {Home-based counselling and testing}, {Linear regression}, {Stigma}, {Sub-Saharan Africa}, {Voluntary Counselling and Testing}, {Zambia}},
	Pages = {18-25},
